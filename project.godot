; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Logic Workbench"
config/version="1.0.1"
run/main_scene="res://scenes/launcher.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Lime Inc/Logic Workbench"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.2", "GL Compatibility")
run/low_processor_mode=true
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/icon.png"

[audio]

buses/default_bus_layout="res://assets/default_bus_layout.tres"

[autoload]

SaveSystem="*res://addons/save_system/save_system.gd"
Global="*res://scripts/global.gd"

[debug]

file_logging/log_path="user://logs/dls2.log"

[display]

window/size/borderless=true
window/per_pixel_transparency/allowed=true

[editor]

naming/node_name_num_separator=2
naming/default_signal_callback_to_self_name="_on_{node_name}_{signal_name}"

[editor_plugins]

enabled=PackedStringArray("res://addons/ReorderableContainer/plugin.cfg", "res://addons/copper_dc/plugin.cfg", "res://addons/explore-editor-theme/plugin.cfg", "res://addons/inspector_extender/plugin.cfg", "res://addons/logger/plugin.cfg", "res://addons/save_system/plugin.cfg", "res://addons/tnowe_extra_controls/plugin.cfg")

